using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

/// <summary>
/// Class1 的摘要描述
/// </summary>
public class Circle
{
    public Circle()
    {
        //
        // TODO: 在這裡新增建構函式邏輯
        //
    }
    public double radius;

    public double Area()
    {
        return Math.PI * radius * radius;
    }
}
