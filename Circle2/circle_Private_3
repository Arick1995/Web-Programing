using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

/// <summary>
/// Circle 的摘要描述
/// </summary>
public class Circle
{
    public Circle()
    {
        //
        // TODO: 在這裡新增建構函式邏輯
        //
    }

    public double Radius
    {
        get;
        set;
    } 

    public double Area()
    {
        return Math.PI * Radius * Radius; 
    }
}
